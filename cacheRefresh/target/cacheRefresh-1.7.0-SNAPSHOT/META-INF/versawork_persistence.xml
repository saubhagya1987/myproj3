<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
	version="2.0">

	<!-- ````````````````````````Persistence Unit for "versawork_DS" database```````````````````````` -->
	<persistence-unit name="versawork_DS"
		transaction-type="RESOURCE_LOCAL">
		<provider>org.hibernate.ejb.HibernatePersistence</provider>
		<non-jta-data-source>java:jboss/datasources/versawork_DS</non-jta-data-source>

		<class>com.versawork.http.model.Account</class>
		<class>com.versawork.http.model.AccountBloodPressureEngage</class>
		<class>com.versawork.http.model.AccountBloodPressureManagementSchedule</class>
		<class>com.versawork.http.model.AccountBloodPressureManagement</class>
		<class>com.versawork.http.model.AccountMedicationEngage</class>
		<class>com.versawork.http.model.AccountMedicationManagement</class>
		<class>com.versawork.http.model.AccountMedicationManagementReminder</class>
		<class>com.versawork.http.model.AccountMedicationManagementSchedule</class>
		<class>com.versawork.http.model.AccountReminderMedicationRelation</class>
		<class>com.versawork.http.model.AccountRole</class>
		<class>com.versawork.http.model.AccountToViewedFacilityNotice</class>
		<class>com.versawork.http.model.ActivityLog</class>
		<class>com.versawork.http.model.AppointmentStatusCodes</class>
		<class>com.versawork.http.model.DatePart</class>
		<class>com.versawork.http.model.EngageAppFeatures</class>
		<class>com.versawork.http.model.AccountBloodPressureEngageResponse</class>
		<class>com.versawork.http.model.EngageAppModules</class>
		<class>com.versawork.http.model.EngageClientToFeature</class>
		<class>com.versawork.http.model.FacilityNotice</class>
		<class>com.versawork.http.model.FacilityProvider</class>
		<class>com.versawork.http.model.FacilityService</class>
		<class>com.versawork.http.model.Frequency</class>
		<class>com.versawork.http.model.MedicationDosage</class>
		<class>com.versawork.http.model.MedicationKind</class>
		<class>com.versawork.http.model.MedicationMethod</class>
		<class>com.versawork.http.model.PatientAppointmentRequest</class>
		<class>com.versawork.http.model.PatientFeedback</class>
		<class>com.versawork.http.model.Response</class>
		<class>com.versawork.http.model.AccountNotificationHistory</class>
		<class>com.versawork.http.model.NotificationType</class>
		<class>com.versawork.http.model.Warnings</class>



		<properties>
			<property name="hibernate.dialect" value="org.hibernate.dialect.SQLServer2005Dialect" />
			<property name="hibernate.show_sql" value="false" />
			<property name="hibernate.validator.apply_to_ddl" value="false" />
			<property name="hibernate.validator.autoregister_listeners"
				value="false" />
			<property name="hibernate.temp.use_jdbc_metadata_defaults"
				value="false" />
			<property name="hibernate.connection.isolation" value="1" />


		</properties>
	</persistence-unit>




</persistence>
